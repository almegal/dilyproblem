{
    "extends": "react-app",
    "env": {
        "browser": true,
        "node": true
    },
    "rules": {
      "no-dupe-keys": [1],
      " no-nested-ternary": [0],
      "no-undef": [1],
      "no-unused-vars": ["error", { "varsIgnorePattern": "style" }],
      "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }]
  }
}
